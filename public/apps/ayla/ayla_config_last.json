                                            }],
                                        "children": []
                                    }, {
                                        "id": "main3",
                                        "name": "main3",
                                        "container": "aylalay",
                                        "styleProps": [{
                                                "selector": "#main3",
                                                "display": "flex",
                                                "flex-flow": "row",
                                                "color": "red",
                                                "flex": "1 0 0px"
                                            }],
                                        "children": []
                                    },
                                    {
                                        "id": "main4",
